{
  "expo": {
    "name": "Door Access",
    "slug": "access-control",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#0D294E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D294E"
      },
      "package": "br.com.fateccarapicuiba.accesscontrol"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["react-native-wifi-reborn"]
  }
}
