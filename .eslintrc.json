{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": ["warn", "single"],
        "semi": ["warn", "never"],
        "indent": ["warn", 2],
        "no-var": ["warn"],
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
}
